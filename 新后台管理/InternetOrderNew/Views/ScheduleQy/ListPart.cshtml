
@{
    Layout = null;
}
@model List<dynamic>
<table class="layui-table layui-form" lay-skin='nob'>
    <thead>
        <tr>     
            <th>企业编号</th>      
            <th>企业名称</th>
            <th>排期时间</th>
            <th>排期号源</th>
            <th>提前天数</th> 
            <th>休息日</th>                    
            <th>预约截止时间</th>
            <th>操作</th>
          
        </tr>
    </thead>
    @if (Model != null && Model.Count > 0)
    {
        foreach (var item in Model)
        {
            <tr class="">
                <td>@item.qybh</td>
                <td>@item.mc</td>
                <td>@item.ksrq.ToString("yyyy-MM-dd") ~ @item.jsrq.ToString("yyyy-MM-dd")</td>
                <td>@item.tjrs</td>                             
                <td>@item.tqts</td>
                <td>@item.xxr</td>                
                <td>@item.jzsj</td>           
                <td><a class="text_blue" href= '@(Url.Content("~/ScheduleQy/edit/")+item.pqbh)'>编辑</a> </td>

            </tr>
        }
    }
</table>
@if (Model == null || Model.Count == 0)
{
    <div class="data_none"><img src="~/content/assets/images/nothing.png" alt=""></div>
}
<!-- 分页样式 -->
@(ViewBag.Pager)