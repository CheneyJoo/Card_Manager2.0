
@{
    Layout = null;
}
@model List<Model.QyygxxModel>
<table class="layui-table layui-form" lay-skin='nob'>
    <thead>    
        <tr>
            <th>企业编号</th>
            <th>企业名称</th>
            <th>员工编号</th>
            <th>套餐名称及组合项</th>
            <th>售价</th>
            <th>姓名</th>
            <th>性别</th>
            <th>电话</th>            
            <th>婚姻状态</th>
            <th>身份证号</th>
            <th>同步时间</th>
            <th>操作</th>         
        </tr>      
    </thead>
   @if (Model != null && Model.Count > 0)
   {
       foreach (var item in Model)
       {
        <tr class="">
            <td>@item.dwbh</td>
            <td>@item.dwmc</td>
            <td>@item.rybh</td>
          
            <td><a  onclick="getMoreInfo('@(item.yybh)','@item.tcbh','@item.tcmc')" class="text_blue" style="cursor:pointer" >@item.tcmc</a></td>

            <td>@item.jg.ToString("0.##")</td>
            <td>@item.xm</td>
            
            @if (item.xb == 0)
            {
                <td>女</td>
            }
            else
            {
                <td>男</td>
            }
            <td>@item.dh</td>
            @if (item.hz == 0)
            {
                <td>未婚</td>
            }
            else
            {
                <td>已婚</td>
            }
          
            <td>@item.sfzh</td>
            <td>@item.createtime.ToString("yyyy-MM-dd HH:mm:ss")</td>

            <td><a class="text_blue" href="javascript:edit('@item.id','@item.xm','@item.mm')">修改密码</a> <a class="text_blue" href="javascript:deletery('@item.id')">删除</a> </td>
          
        </tr>
       }
   }
</table>
@if (Model == null || Model.Count == 0)
{
    <div class="data_none"><img src="~/content/assets/images/nothing.png" alt=""></div>
}
<!-- 分页样式 -->
@(ViewBag.Pager)